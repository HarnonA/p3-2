import java.util.InputMismatchException;


public class IFace {
	Scanner s = new Scanner();
	
	public static void main(String[] args) {
		
	}
	
	private int lerInt(){
		int num = 0;
		try {
			num = s.nextInt();
		} catch (InputMismatchException e) {
			System.out.println("Digite um valor valido!");
			s.nextLine();
			num = -1;
		}
		return num;
		
	}

}
