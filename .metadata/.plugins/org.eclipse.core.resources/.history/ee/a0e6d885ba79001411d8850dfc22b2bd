package elements;

public class BuildPlan {

	private Item[] elements;

	public BuildPlan() {
		// create new Array of elements
		this.elements = new Item[] { new Invocation(), new Invocation(),
				new Invocation(), new Invocation(), new Body(), new Engine() };
	}

	public void getElements() {
		for (Item item : elements) {
			item.accept(visitor);
		}
		visitor.visit(this);
		
	}
}
